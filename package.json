{
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "babel-jest": "^30.2.0",
    "cross-env": "^7.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  },
  "name": "case-eaters",
  "version": "1.0.0",
  "description": "'''bash",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:coverage",
    "test:win": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=./backend/.env.test NODE_OPTIONS=--experimental-vm-modules node -r dotenv/config ./node_modules/jest/bin/jest.js --detectOpenHandles --coverage",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasbenti/Case-Eaters.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thomasbenti/Case-Eaters/issues"
  },
  "homepage": "https://github.com/thomasbenti/Case-Eaters#readme",
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "backend/**/*.js",
      "!backend/**/node_modules/**",
      "!backend/config/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
